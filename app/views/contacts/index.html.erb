		<div class="span3">
			<%= link_to image_tag("kokonaismyynti_pieni.jpg" , :alt => "Kokonaismyynti", :mouseover => "kokonaismyynti_pieni_hover.JPG"), kokonaismyynti_path %>
		</div>	
		<div class="span3">
			<%= link_to image_tag("myyntiryhmat_pieni.jpg" , :alt => "Myyntiryhmät", :mouseover => "myyntiryhmat_pieni_hover.JPG"), myyntiryhmat_path %>

<div class="span2" id="vasenpalkki">
<%= render 'myyja_shared/vasenpalkki' %>
</div>
<div class="span10">
	<div class="row">
		<div class="span3 offset1">
			<%= image_tag "myynti_iso.JPG" %>
		</div>
		<div class="span2">
			<%= link_to image_tag("kilpailut_pieni.jpg", :mouseover => "kilpailut_pieni_hover.JPG"), kilpailut_path %>
		</div>
		<div class="span2">
			<%= image_tag "myyja_placeholder_pieni.jpg" %>
		</div>
		<div class="span3" id="oikeapalkki">
			<%= render 'myyja_shared/oikeapalkki'%>
		</div>
	</div>
	<div class="row-fluid">
		<div class="span4">
			<div class="myyntipv">
				<div class="row">
					<h3>Tänään</h3><br />
				</div>
				<div id="tanagraph">
				 	<%= render :partial => 'shared/graph_tanaan', :locals => { :myyja => current_user, :width => 84, :height => 300} %>
				 </div>
			</div>
		</div>
		<div class="span4">
			<div class="myyntikk">
				<div class="row">
					<h3>Tässä kuussa</h3><br />
				</div>
				<div id="kkgraph">
					<%= render :partial => 'shared/graph_kuukausi', :locals => { :myyja => current_user, :width => 84, :height => 300} %>
				</div>
			</div>
		</div>
		<div class="span4">
			<div class="osaamisluvut" id="osaamisluvut">
				<%= render 'myyja_shared/osaamisluvut'%>
			</div>
		</div>
	</div>	
</div>
	
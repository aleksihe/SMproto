<%= @aika %>
<%= @myyja_valittu.nimi %>
<% if @aika == "Tänään" %>
 <%= render :partial => 'shared/graph_tanaan', :locals => { :myyja => @myyja_valittu, :width => 50, :height => 200} %>
<% elsif @aika == "Tämä kuu" %>
 <%= render :partial => 'shared/graph_kuukausi', :locals => { :myyja => @myyja_valittu, :width => 50, :height => 200} %>
<% end %>